#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_jest-cli@24.9.0@jest-cli/bin/jest.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../_jest-cli@24.9.0@jest-cli/bin/jest.js" "$@"
  ret=$?
fi
exit $ret
